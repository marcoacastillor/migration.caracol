BROKER SCHEMA co.com.caracol.consultatalentoxnombre

DECLARE ns1 NAMESPACE 'http://www.caracoltv.com.co/mercadeo/indicetalento/consultatalentoxnombre/';

CREATE COMPUTE MODULE ConsultaTalentoXNombre_ResponseService
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		--Copiar cabeceras de respuesta
		SET OutputRoot.MQMD 	= Environment.usr._header.MQMD;
		SET OutputRoot.MQRFH2 	= Environment.usr._header.MQRFH2;
		
		--Declarar variable referencia de entrada
		DECLARE refInp REFERENCE TO InputRoot;
		MOVE refInp LASTCHILD;
		MOVE refInp LASTCHILD;
		
		IF EXISTS(refInp.*:BuscarTalentoXNombreResult[]) THEN
			MOVE refInp LASTCHILD;
			--Crear mensaje de salida
			CREATE FIELD OutputRoot.XMLNSC.ns1:BuscarTalentoXNombreResponse;
			DECLARE refOut REFERENCE TO OutputRoot.XMLNSC.ns1:BuscarTalentoXNombreResponse;
		
			DECLARE i INTEGER 1;
			FOR refReqs AS refInp.*:Actor[]  DO
				
				SET refOut.Actor[i].TalentoCodigo	        	= FIELDVALUE(refReqs.*:TalentoCodigo);
				SET refOut.Actor[i].Foto	        			= FIELDVALUE(refReqs.*:Foto);
				SET refOut.Actor[i].NombreCompleto	        	= FIELDVALUE(refReqs.*:NombreCompleto);
				SET refOut.Actor[i].RutaPdfUltimoEstudio	    = FIELDVALUE(refReqs.*:RutaPdfUltimoEstudio);
				SET refOut.Actor[i].TalentoId	       			= FIELDVALUE(refReqs.*:TalentoId);
				
				SET i = i + 1;
			END FOR;
		ELSE
			THROW USER EXCEPTION CATALOG 'CARACOL.EXCEPTION.CUSTOM' MESSAGE 27 VALUES('CUST::ERROR BACKEND');
		END IF;
		RETURN TRUE;
	END;
END MODULE;